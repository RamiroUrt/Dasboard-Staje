{
  "version": 2,
  "builds": [
    {
      "src": "api/links.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/3y4",
      "dest": "/public/periodo3y4.html?categoria=3y4"
    },
    {
      "src": "/4y5",
      "dest": "/public/periodo4y5.html?categoria=4y5"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/links",
      "dest": "/api/links",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/404.html"
    }
  ]
}
